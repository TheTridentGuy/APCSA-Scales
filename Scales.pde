void draw_image(int[][][] image, int x, int y, int pixel_size){
  int[] new_color = {(int)(Math.random()*255), (int)(Math.random()*255), (int)(Math.random()*255)};
  for (int pixel_x=0; pixel_x<image.length; pixel_x++){
